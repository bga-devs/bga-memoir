{OVERALL_GAME_HEADER}
<div id="m44-container">
  <div class="m44-hrule"></div>
  <div id="m44-top-part">
    <div id="scenario-informations"></div>
    <div id="top-player"></div>
  </div>

  <div id="m44-central-part">
    <div id="deck-holder"></div>
    <div id="m44-board-wrapper">
      <div id="m44-board">
        <div id="m44-board-terrains" class='m44-grid'></div>
        <div id="m44-board-labels" class='m44-grid'></div>
        <div id="m44-board-units" class='m44-grid'></div>
      </div>
    </div>
  </div>

  <div id="m44-bottom-part">
    <div id="bottom-player"></div>
    <div id="hand"></div>
  </div>
</div>
{OVERALL_GAME_FOOTER}
